.pasdiv {
  font-family: Poppins, sans-serif;
  font-style: normal;
  font-weight: 400;
  font-size: 14px;
  line-height: 22px;
  margin-bottom: 10px;
  position: relative;
  cursor: pointer;
  font-weight: bold;
}
.pasdiv a {
  color: black;
  text-decoration: none;
}

.pasdiv::after {
  content: "";
  position: absolute;
  width: 55%;
  height: 1.5px;
  background-color: black;
  left: 0%;
  bottom: 0%;
  transform: scale(0, 1);
  transform-origin: 0% 100%;
  transition: transform 0.4s ease;
}
.pasdiv:hover::after {
  transform: scale(1, 1);
}

